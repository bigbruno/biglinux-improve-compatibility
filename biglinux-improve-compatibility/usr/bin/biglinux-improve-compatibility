#!/bin/bash

ln -sf "/usr/lib/modules/$(uname -r)/build" /usr/src/linux

# if folder not empty
if [ ! -z "$(ls -A -- /etc/init.d)" ]; then

    #run loop
    for i  in /etc/init.d/*; do
        $i start
    done

fi


